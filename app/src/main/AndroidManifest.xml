<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ehernandez.asesoruanl" >

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

    <application
        android:name=".ParseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Base.AppTheme.NoActionBar">

        <activity
            android:name=".OverviewActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".Register"
            android:screenOrientation="portrait"/>

        <activity android:name=".Login"
            android:screenOrientation="portrait"/>

        <activity android:name=".Signup"
            android:screenOrientation="portrait"/>

        <activity android:name=".PersonalInformationAsesor"
            android:screenOrientation="portrait"/>

        <activity android:name=".MyConsultancies"
            android:screenOrientation="portrait"/>

        <activity android:name=".SearchByConsultancy"
            android:screenOrientation="portrait"/>

        <activity android:name=".SearchConsultancy"
            android:screenOrientation="portrait"/>

        <activity android:name=".PersonalInformationStudent"
            android:screenOrientation="portrait"/>

        <activity android:name=".AddAsesorConsultancy"
            android:screenOrientation="portrait"/>

        <activity android:name=".AddStudentConsultancy"
            android:screenOrientation="portrait"/>

        <activity android:name=".EditConsultancy"
            android:screenOrientation="portrait"/>

        <activity android:name=".SendEmail"
            android:screenOrientation="portrait"/>

        <activity android:name=".Settings"
            android:screenOrientation="portrait"/>

        <activity android:name=".ProfileSettings"
            android:screenOrientation="portrait"/>

        <activity android:name=".PasswordSettings"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
